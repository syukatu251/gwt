<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="com.ry.gwt.client.MainComposite.MyStyle">
	body{ font-size:18px; color:#FFF; }
.boxShadow { border:solid 1px #2d2d2d;  text-align:center; background:#575757; padding:50px 50px 50px 50px;  -moz-border-radius: 5px;  -webkit-border-radius: 5px; border-radius: 5px;}
.boxShadow a {
	color:#FFF

	}
		.boxShadowOff {
			-moz-box-shadow: 0px 0px 5px #000000;
			-webkit-box-shadow: 0px 0px 5px #000000;
			box-shadow: 0px 0px 5px #000000;
		}
		.boxShadowOn {
	-moz-box-shadow:inset 10px 10px 5px #000000;
-webkit-box-shadow:inset 10px 10px 5px #000000;
box-shadow:inset 10px 10px 5px #000000;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="verticalPanel"/>
	</g:HTMLPanel>
</ui:UiBinder> 